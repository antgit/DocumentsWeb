@using Stimulsoft.Report.MvcDesign
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/PrintForms.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
    @Html.StiMvcDesignerRenderScripts()
</head>
<body>
    <div class="page">
        <div id="main">
            @*@RenderBody()*@
            @Html.ActionLink("New Report", "NewReport")
            <br />Create a new report template

            <br /><br />
            @Html.ActionLink("Edit Report", "EditReport")
            <br />Edit the SimpleList.mrt report template
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
